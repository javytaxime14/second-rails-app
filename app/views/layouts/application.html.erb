  <body>
    <div id="page-wrapper">

				<div id="header">
				<h1><a href="index.html" id="logo">Helios</a></h1>
					
						<nav id="nav">
							<ul>
								<li><%= link_to 'Index', pages_index_path %></li>
								<li><%= link_to 'Test', pages_test_path %></li>
